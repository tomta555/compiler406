def find_min(a:int[],min:int):int{
	i:int;
	i = 0 ;	
	while(i < 13){
		if(a[i]<min){
			min = a[i];	
		}
		i=i+1 ;
	}
return min;
}
def find_max(a:int[],max:int):int{
	range:int; 
	range = length(a);
	i:int; 
	i = 0 ;	
	while(i < range){
		if(a[i]>max){
			max = a[i];	
		}
		i=i+1;
	}
return max;
}
